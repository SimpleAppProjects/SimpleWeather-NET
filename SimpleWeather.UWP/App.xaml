                    <Style
                        x:Key="HyperlinkButtonStyle"
                        BasedOn="{StaticResource DefaultHyperlinkButtonStyle}"
                        TargetType="HyperlinkButton">
                        <Setter Property="Foreground" Value="{ThemeResource HyperlinkButtonForeground}" />
                    </Style>
                    <SolidColorBrush x:Key="SnackbarForegroundBrush" Color="{ThemeResource HyperlinkButtonForeground}" />
