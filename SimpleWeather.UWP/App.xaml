                    <StaticResource x:Key="RegionColor" ResourceKey="SystemColorWindowColor" />
                    <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
                    <StaticResource x:Key="AppColor" ResourceKey="SimpleBlue" />
                    <StaticResource x:Key="AppAccentColor" ResourceKey="SimpleBlue" />
                    <SolidColorBrush x:Key="AppColorBrush" Color="{ThemeResource AppColor}" />
