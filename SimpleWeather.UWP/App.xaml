            <!--  Converters Start  -->
            <toolkitConverters:BoolNegationConverter x:Key="inverseBoolConverter" />
            <helpers:ColorToSolidColorBrushConverter x:Key="color2SolidColorConverter" />
            <helpers:ColorToSolidColorBrushConverter x:Key="solidColorBrush2ColorConverter" IsInverse="True" />
            <helpers:BooleanToGridLengthConverter x:Key="bool2GridLengthConverter" />
            <helpers:BooleanToGridLengthConverter x:Key="inverseBool2GridLengthConverter" IsInverse="True" />
